{
    "name": "chrisjk123/laravel-social",
    "description": "",
    "type": "library",
    "authors": [
        {
            "name": "Christopher Kelker",
            "email": "christopherjk123@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "readme": "README.md",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^9.0",
        "laravel/socialite": "^5.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0|^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Chriscreates\\Social\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chriscreates\\Social\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"

    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Chriscreates\\Social\\Providers\\SocialServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
